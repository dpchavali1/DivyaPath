<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base layer -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFF8E1" />
        </shape>
    </item>
    <!-- Subtle decorative ring pattern -->
    <item android:gravity="center">
        <shape android:shape="oval">
            <size android:width="200dp" android:height="200dp" />
            <stroke android:width="0.5dp" android:color="#15FF9800" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <size android:width="160dp" android:height="160dp" />
            <stroke android:width="0.5dp" android:color="#10FF9800" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <size android:width="120dp" android:height="120dp" />
            <stroke android:width="0.5dp" android:color="#0DFF9800" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <size android:width="80dp" android:height="80dp" />
            <stroke android:width="0.5dp" android:color="#0AFF9800" />
        </shape>
    </item>
</layer-list>
